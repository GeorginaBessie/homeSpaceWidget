{
  "manifest_version": 2,

  "name": "Home.Space",
  "description": "Finding a suitable rental",
  "version": "1.0",

  "browser_action": {
    "default_icon": "homeIcon32x32.png",
    "default_title": "Check additional info",
    "default_popup": "popup.html"
  },

  "permissions": [
    "declarativeContent",
    "tabs",
    "activeTab",
    "https://home-space.s3.amazonaws.com/"
  ]
}